<div class="sign-wrapper">
  <div class="sign-box">
    <%= simple_form_for @plant do |f| %>
    <%= f.input :name %>
    <br>
    <%= f.input :description %>
    <br>
    <%= f.input :address %>
    <br>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <br>
    <%= f.submit "Modify", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
